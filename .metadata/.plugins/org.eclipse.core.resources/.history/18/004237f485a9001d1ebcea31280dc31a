package curso_programacao;

public class Exercicio2_01 {

	public static void main(String[] args) {

		int num = -10;
		
		//FUNCAO TERNARIA
		//System.out.println((num < 0) ? "NEGATIVO" : "NAO NEGATIVO");		
		
		//FUNCAO NORMAL
		if (num < 0) {
			System.out.println("NEGATIVO");
		}
		else {
			System.out.println("NAO NEGATIVO");
		}

			
	}
}
